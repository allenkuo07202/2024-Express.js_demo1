<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <meta name="author" content="Allen Kuo" />
    <link rel="stylesheet" href="/style.css" />
  </head>
  <body>
    <table>
      <tr>
        <th>名稱</th>
        <th>評價</th>
        <th>熱門程度</th>
        <th>流行度</th>
      </tr>
      <% languages.forEach(lang => { %>
      <tr>
        <td><%= lang.name %></td>
        <td><%= lang.rating %></td>
        <td><%= lang.popularity %></td>
        <td><%= lang.trending %></td>
      </tr>
      <%}); %>
    </table>
  </body>
</html>
